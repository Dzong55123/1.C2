count = 0

while count < 2:
    print("INPUT: Please enter two integers separated by space:")
    numbers = input().split()
    num1 = int(numbers[0])
    num2 = int(numbers[1])
    
    if num1 > num2:
        print(f"{num1} is greater than {num2}")
    elif num1 < num2:
        print(f"{num1} is less than {num2}")
    else:
        print(f"{num1} is equal to {num2}")

    count += 1

print("END")
